{
  "name": "notes",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "clean": "rm -r build/*",
    "build-server": "babel . -d build/ --ignore node_modules/,public/ --presets es2015,react",
    "start": "node ./bin/www",
    "build": "babel ./bin/www --out-file ./build/www",
    "transpile": "babel . --presets es2015,react -d build/ --ignore node_modules/,public/",
    "compile": "npm run transpile && npm run build && npm start"
  },
  "dependencies": {
    "body-parser": "~1.13.2",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "express": "^4.13.4",
    "express-react-views": "^0.10.2",
    "jade": "~1.11.0",
    "marked": "^0.3.5",
    "mongoose": "^4.4.12",
    "morgan": "~1.6.1",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "serve-favicon": "~2.3.0",
    "simplemde": "^1.10.1"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.13.2",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1"
  }
}
