{
  "name": "notes",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "debug": "node $NODE_DEBUG_OPTION ./node_modules/babel-cli/bin/babel-node.js ./bin/www --presets es2015,react",
    "resources": "cp -r bin out/ && cp -r public out/ && cp -r views out/",
    "out-bin": "babel bin --out-dir out/bin --source-maps",
    "out-models": "babel models --out-dir out/models --source-maps",
    "out-routes": "babel routes --out-dir out/routes --source-maps",
    "out-util": "babel util --out-dir out/util --source-maps",
    "out-app": "babel app.js --out-file out/app.js --source-maps",
    "babel": "npm run resources && npm run out-models && npm run out-routes && npm run out-util && npm run out-app"
  },
  "dependencies": {
    "body-parser": "~1.13.2",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "express": "^4.13.4",
    "express-react-views": "^0.10.2",
    "mongoose": "^4.4.12",
    "morgan": "~1.6.1",
    "react": "^15.1.0",
    "serve-favicon": "~2.3.0",
    "simplemde": "^1.10.1"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.13.2",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1"
  }
}
